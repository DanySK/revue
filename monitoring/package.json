{
  "type": "module",
  "name": "monitoring",
  "version": "0.0.0",
  "private": true,
  "description": "Subproject of revue",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:client && npm run start:server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "run-p type-check build-only",
    "start:client": "npm run build:client && vite --config client/vite.config.js",
    "start:server": "npm run build:server && node --experimental-specifier-resolution=node dist/server/src/index.js",
    "build:server": "tsc -p server/tsconfig.json",
    "type-check": "vue-tsc -p client/tsconfig.json --noEmit --composite false",
    "build-only": "vite build --config client/vite.config.js",
    "watch:server": "tsc -p server/tsconfig.json -w & nodemon --experimental-specifier-resolution=node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mala1180/revue.git"
  },
  "keywords": [],
  "author": "Mattia Matteini, Alberto Paganelli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mala1180/revue/issues"
  },
  "homepage": "https://github.com/Mala1180/revue#readme",
  "dependencies": {
    "domain": "file:../domain",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.1",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "nodemon": "^3.0.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}
